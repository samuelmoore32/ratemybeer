<div id="homeBeerCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#homeBeerCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#homeBeerCarousel" data-slide-to="1"></li>
            <li data-target="#homeBeerCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides 8888888aqw--> 
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="~/img/car1.jpg" alt="BeerInGlasses">
                <div class="container">
                    <div class="carousel-caption" style="text-align:left">
                        <h1><strong>Contact Us</strong></h1>
                        <p class="lead">Click Below</p>
                        <a>@Html.ActionLink("Contact Us >>", "ContactUs", "Home", null, new { @class = "btn btn-primary btn-large" })</a>
                    </div>
                </div>
            </div>

            <div class="item">
                <img src="~/img/car2.jpg" alt="Bottles">
                <div class="container">
                    <div class="carousel-caption" style="text-align:left">
                        <h1><strong>FAQ</strong></h1>
                        <p class="lead">Frequently Asked Questions</p>
                        <a>@Html.ActionLink("FAQ >>", "FAQ", "Home", null, new { @class = "btn btn-primary btn-large" })</a>
                    </div>
                </div>
            </div>

            <div class="item">
                <img src="~/img/car3.jpg" alt="Hops">
                <div class="container">
                    <div class="carousel-caption" style="text-align:left">
                        <h1><strong>Looking For The DL on a Beer?</strong></h1>
                        <p class="lead">Just click the button below to find what you seek..</p>
                        <a>@Html.ActionLink("Search >>", "SearchPage", "Search", null, new { @class = "btn btn-primary btn-large" })</a>
                    </div>
                </div>
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#homeBeerCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#homeBeerCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
</div>


<div class="row">
    <div class="col-md-4">
        <h2>Follow Us On Facebook</h2>
        <div class="fb-page" data-href="https://www.facebook.com/ratebeer/" data-tabs="timeline" data-small-header="true" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true">
        <blockquote cite="https://www.facebook.com/ratebeer/" class="fb-xfbml-parse-ignore">
            <a href="https://www.facebook.com/ratebeer/">RateBeer</a></blockquote></div>
    </div>
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
        <h2>Follow us on Twitter</h2>

        <a class="twitter-timeline" data-height="600" href="https://twitter.com/ratebeer">Tweets by TwitterDev</a>
        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
        <div id="fb-root"></div>
        <script>
            (function(d, s, id) {
              var js, fjs = d.getElementsByTagName(s)[0];
              if (d.getElementById(id)) return;
              js = d.createElement(s); js.id = id;
              js.src = "//connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v2.7";
              fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
    </div>
</div>